{
  "name": "ai-elearning-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run kill-ports && npm run start-all",
    "dev-frontend-only": "next dev",
    "kill-ports": "pkill -f 'uvicorn' || true && pkill -f 'next dev' || true && sleep 2",
    "start-backend": "cd ../backend && python3 -m uvicorn app.main:app --host 0.0.0.0 --port 8000 --reload",
    "start-all": "concurrently \"npm run start-backend\" \"sleep 5 && next dev\"",
    "build": "next build",
    "start": "next start",
    "gcp-build": "NODE_ENV=production next build",
    "lint": "next lint",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "type-coverage": "npx type-coverage --ignore-catch"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@next-auth/mongodb-adapter": "^1.1.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-slot": "^1.1.7",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@types/prismjs": "^1.26.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.2.0",
    "lucide-react": "^0.522.0",
    "next": "^14.2.30",
    "next-auth": "^4.24.11",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "react-youtube": "^10.1.0",
    "recharts": "^3.0.0",
    "refractor": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^2.5.4",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-youtube": "^7.6.2",
    "autoprefixer": "^10.4.19",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
